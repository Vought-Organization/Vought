{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:react/recommended", "airbnb", "prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "react/jsx-no-useless-fragment": "off",
    "no-console": "error",
    "no-unused-vars": "warn",
    "react/function-component-definition": "airbnb",
    "import/named": 1
  }
}
